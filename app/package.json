{
  "name": "epamLib",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build": "tsc -w",
    "test": "jest",
    "lint": "tslint 'src/**/*.ts?(x)' --format stylish",
    "type-sync": "typesync",
    "postinstall": "typesync",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "tsc -p . && lint-staged",
    "check-env": "solidarity",
    "save-env": "solidarity snapshot",
    "android-linux": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android"
  },
  "dependencies": {
    "mobx": "^4.3.0",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^5.1.2",
    "react": "16.8.3",
    "react-native": "0.59.0",
    "react-native-elements": "^0.19.1",
    "react-native-fbsdk": "^0.8.0",
    "react-native-input-scroll-view": "^1.9.1",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-super-grid": "^3.0.3",
    "react-native-vector-icons": "4.5.0",
    "react-navigation": "^2.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/jest": "^23.3.2",
    "@types/prettier": "^1.13.2",
    "@types/react": "^16.4.14",
    "@types/react-addons-test-utils": "^0.14.22",
    "@types/react-dom": "^16.0.7",
    "@types/react-native": "^0.56.18",
    "@types/react-native-fbsdk": "^0.6.3",
    "@types/react-native-vector-icons": "^4.6.3",
    "@types/react-navigation": "^2.0.20",
    "babel-jest": "22.4.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "4.0.0",
    "husky": "^0.14.3",
    "jest": "22.4.4",
    "lint-staged": "^7.1.2",
    "mobx-logger": "^0.7.0",
    "npm-run-all": "^4.1.3",
    "nyr": "^1.1.0",
    "prettier": "^1.12.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.0",
    "react-native-dotenv": "^0.1.1",
    "react-native-typescript-transformer": "^1.2.6",
    "solidarity": "^2.1.0",
    "solidarity-react-native": "^2.0.2",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3",
    "typesync": "^0.2.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
